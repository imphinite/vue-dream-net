{
  "name": "vue-dream-net",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --config build/webpack.dev.js",
    "build": "webpack --config build/webpack.prod.js",
    "lint": "vue-cli-service lint --no-fix --ext .js,.vue src",
    "lint-fix": "vue-cli-service lint --fix --ext .js,.vue src",
    "unit": "jest --config test/unit/jest.conf.js --runInBand --coverage",
    "babel": "babel --presets es2015 js/main.js -o build/main.build.js",
    "webpack": "webpack"
  },
  "dependencies": {
    "core-js": "^3.3.2",
    "lodash": "^4.17.15",
    "vue": "^2.6.10",
    "vuetify": "^2.1.0",
    "vuex": "^3.1.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "file-loader": "^4.2.0",
    "jest": "^24.9.0",
    "jest-serializer-vue": "^2.0.2",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "url-loader": "^2.2.0",
    "vue-cli-plugin-vuetify": "^2.0.2",
    "vue-jest": "^3.0.5",
    "vue-loader": "^15.7.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.3.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
